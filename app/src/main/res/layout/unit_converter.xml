<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/main_root"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/toolbar"
			android:layout_width="match_parent"
			android:layout_height="56dp"
			android:background="#0377BD"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview6"
				android:layout_width="56dp"
				android:layout_height="56dp"
				android:src="@drawable/ic_arrow_back_white"
				android:scaleType="center"/>
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="Length"
				android:textSize="20sp"
				android:textStyle="bold"
				android:textColor="#FFFFFF"
				android:layout_weight="1"/>
			<ImageView
				android:id="@+id/imageview7"
				android:layout_width="56dp"
				android:layout_height="56dp"
				android:src="@drawable/ic_search_white"
				android:scaleType="center"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/main_conv_layout"
			android:layout_width="match_parent"
			android:layout_height="250dp"
			android:background="#0377BD"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/conv_layout_first"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/linear32"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview15"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="12dp"
						android:layout_marginRight="12dp"
						android:text="From"
						android:textSize="20sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1"/>
					<EditText
						android:id="@+id/edittext1"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:background="@android:color/transparent"
						android:text="1"
						android:textSize="30sp"
						android:textColor="#FFFFFF"
						android:textColorHint="#607D8B"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear31"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="bottom"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview16"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="12dp"
						android:layout_marginRight="12dp"
						android:layout_marginBottom="2dp"
						android:text="Farenheit"
						android:textSize="20sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1"/>
					<TextView
						android:id="@+id/textview17"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginBottom="2dp"
						android:text="°F"
						android:textSize="25sp"
						android:textColor="#FFFFFF"/>
					<ImageView
						android:id="@+id/imageview10"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:layout_marginBottom="4dp"
						android:src="@drawable/ic_drop_down_18"
						android:scaleType="center"/>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/div"
				android:layout_width="match_parent"
				android:layout_height="2dp"
				android:layout_marginLeft="12dp"
				android:layout_marginRight="12dp"
				android:background="#FFFFFF"
				android:orientation="horizontal"/>
			<LinearLayout
				android:id="@+id/conv_layout_second"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:orientation="vertical"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/linear37"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview23"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="12dp"
						android:layout_marginRight="12dp"
						android:text="To"
						android:textSize="20sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1"/>
					<EditText
						android:id="@+id/edittext3"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:background="@android:color/transparent"
						android:text="0"
						android:textSize="30sp"
						android:textColor="#FFFFFF"
						android:textColorHint="#607D8B"/>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear38"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:gravity="bottom"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/textview21"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginLeft="12dp"
						android:layout_marginRight="12dp"
						android:layout_marginBottom="2dp"
						android:text="Farenheit"
						android:textSize="20sp"
						android:textColor="#FFFFFF"
						android:layout_weight="1"/>
					<TextView
						android:id="@+id/textview22"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginBottom="2dp"
						android:text="°F"
						android:textSize="25sp"
						android:textColor="#FFFFFF"/>
					<ImageView
						android:id="@+id/imageview12"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginRight="8dp"
						android:layout_marginBottom="4dp"
						android:src="@drawable/ic_drop_down_18"
						android:scaleType="center"/>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/top_pad"
			android:layout_width="match_parent"
			android:layout_height="90dp"
			android:background="#EAEFF2"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="80dp"
				android:layout_height="64dp"
				android:src="@drawable/ic_view_list_white"
				android:scaleType="center"/>
			<ImageView
				android:id="@+id/share"
				android:layout_width="80dp"
				android:layout_height="64dp"
				android:src="@drawable/ic_share_white"
				android:scaleType="center"/>
			<ImageView
				android:id="@+id/swap"
				android:layout_width="80dp"
				android:layout_height="64dp"
				android:src="@drawable/ic_swap_vert_white"
				android:scaleType="center"/>
			<ImageView
				android:id="@+id/del"
				android:layout_width="80dp"
				android:layout_height="64dp"
				android:src="@drawable/ic_backspace_white"
				android:scaleType="center"/>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/main_dpad"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:background="#EAEFF2"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/dpad"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="#FBFBFB"
				android:orientation="vertical"
				android:layout_weight="1">
				<LinearLayout
					android:id="@+id/linear11"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/num_1"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview2"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="1"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_2"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview5"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="2"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_3"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview6"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="3"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear10"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/num_4"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview7"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="4"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_5"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview4"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="5"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_6"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview8"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="6"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear12"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/num_7"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview9"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="7"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_8"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview3"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="8"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_9"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview11"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="9"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear13"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="horizontal"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/plus_minus"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview12"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="+/-"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/num_0"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview13"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="0"
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
					<LinearLayout
						android:id="@+id/point"
						android:layout_width="80dp"
						android:layout_height="match_parent"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="horizontal">
						<TextView
							android:id="@+id/textview14"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="."
							android:textSize="25sp"
							android:textColor="#727272"/>
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/sidepad"
				android:layout_width="80dp"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/clear"
					android:layout_width="80dp"
					android:layout_height="85dp"
					android:src="@drawable/ic_clear_white"
					android:scaleType="center"/>
				<LinearLayout
					android:id="@+id/linear9"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:orientation="vertical"
					android:layout_weight="1"/>
				<ImageView
					android:id="@+id/copy"
					android:layout_width="80dp"
					android:layout_height="85dp"
					android:src="@drawable/ic_content_copy_white"
					android:scaleType="center"/>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
